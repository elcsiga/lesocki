{
  "name": "lesocki",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "build": "tsc",
    "postinstall": "npm run ui:install",
    "prestart": "npm run ui && npm run build",
    "start": "node dist/src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ui:install": "cd ui && npm install",
    "ui:clean": "rimraf public",
    "ui:build": "cd ui && npm run build",
    "ui:copy": "copyfiles -u 2 \"ui/public/**/*.*\" public",
    "ui": "npm run ui:clean && npm run ui:build && npm run ui:copy"
  },
