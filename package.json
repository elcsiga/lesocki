    "express-ws": "^5.0.2",
    "ws": "^8.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-ws": "^3.0.1",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.2.2",
